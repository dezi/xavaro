{
  "manifest": {

    "label": "Fernsehprogramm",
    "appname": "tvguide",
    "appicon": "tvguide_175x175.png",
    "appserver": "epg.xavaro.de",

    "files": [
      "main.js"
    ],

    "permissions": [
      "localdomstorage",
      "gps"
    ],

    "cachedefs": [
      { "interval": 24, "pattern": ".*\\.js" },
      { "interval":  1, "pattern": ".*\\.png" },
      { "interval":  1, "pattern": ".*\\.json" },
      { "interval":  1, "pattern": ".*\\.json\\.gz" }
    ]
  }
}
