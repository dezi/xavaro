{
  "manifest": {

    "label": "Blockspiel",
    "appname": "blockgame",
    "appicon": "blockgame_512x512.png",
    "preferences": false,
    "category": "games",
    "events": false,
    "voice": true,

    "permissions": [
      "request",
      "utility"
    ],

    "weblibs": [
      "touch",
      "simple",
      "strings",
      "dialog"
    ],

    "preload": [
      "eval.js"
    ],

    "locale": [
      "strings.json",
      "strings.de-rDE.json"
    ],

    "cachedefs": [
      { "interval": 24, "pattern": ".*.mp3" },
      { "interval": 24, "pattern": ".*.ogg" }
    ]
  }
}
