{
  "manifest": {

    "label": "Testing",
    "appname": "testing",
    "appicon": "testing_512x512.png",

    "preload": [
      "util.js"
    ],

    "files": [
      "main.js",
      "util.js"
    ],

    "permissions": [
      "localdomstorage",
      "gps"
    ],

    "cachedef": [
      { "pattern": ".*\\.js", "intervall": 24 },
      { "pattern": ".*\\.png", "intervall": 24 },
      { "pattern": ".*current\\.json\\.gz", "intervall": 24 }
    ]
  }
}
