{
  "manifest": {

    "label": "Test",
    "appname": "testing",
    "appicon": "testing_225x225.png",
    "appserver": "epg.xavaro.de",
    "preferences": false,

    "permissions": [
      "intercept",
      "request",
      "utility",
      "health",
      "events",
      "media",
      "speak",
      "prefs"
    ],

    "weblibs": [
      "simple",
      "touch"
    ],

    "preload": [
    ],

    "cachedefs": [
      { "interval": 24, "pattern": ".*\\.js" },
      { "interval": 72, "pattern": ".*\\.png" },
      { "interval": 24, "pattern": ".*\\.json" },
      { "interval": 24, "pattern": ".*\\.json\\.gz" }
    ]
  }
}
