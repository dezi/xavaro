{
  "manifest": {

    "label": "Testing",
    "appname": "testing",
    "appicon": "testing_512x512.png",

    "preload": [
      "util.js"
    ],

    "files": [
      "main.js",
      "util.js"
    ],

    "permissions": [
      "localdomstorage",
      "gps"
    ],

    "cachedefs": [
      { "interval": 24, "pattern": ".*\\.js" },
      { "interval":  1, "pattern": ".*\\.png" },
      { "interval":  1, "pattern": ".*\\.json" },
      { "interval":  1, "pattern": ".*\\.json\\.gz" }
    ]
  }
}
