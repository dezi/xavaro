{
  "manifest": {

    "label": "Einkaufszettel",
    "appname": "shoppinglist",
    "appicon": "shoppinglist_512x512.png",
    "appserver": "epg.xavaro.de",
    "voice": true,

    "permissions": [
      "speak",
      "prices",
      "request",
      "utility"
    ],

    "weblibs": [
      "simple",
      "touch",
      "strings",
      "dialog"
    ],

    "files": [
      "main.js"
    ],

    "locale": [
      "strings.json",
      "strings.de-rDE.json"
    ],

    "cachedefs": [
      { "interval": 24, "pattern": ".*\\.js" },
      { "interval": 72, "pattern": ".*\\.png" },
      { "interval": 24, "pattern": ".*\\.json" },
      { "interval": 99, "pattern": "http://epg.xavaro.de/prodata/.*.csv.gzbin" }
    ]
  }
}
