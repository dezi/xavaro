  "de.tube8.com": {
    "url": "http://de.tube8.com/",
    "icon": "http://a3-images.myspacecdn.com/images03/1/cb9e1e694ca941abaf62f0026d18049f/300x300.jpg",
    "label": "TUBE8",
    "default": true,
    "subtype": "erotics",

    "subdomains": [ "tube8.com", "phncdn.com" ],

    "webguard": {

      //
      // Preset age check and table style.
      //

      "cookies": [ "t8disclaimer=1", "platform=tablet" ],

      "mingle": {
        "replace": [

          //
          // Tube8 m.f. re-inserts this div if gone. So set to display none.
          //

          {
            "regex": "(?s)<div id=\"PornhubNetworkBar\">",
            "replace": "<div id=\"PornhubNetworkBar\" style=\"display:none\">"
          },

          //
          // Eliminate undesired links from menu bar.
          //

          {
            "regex": "(?s)<li class=\"float-left\">.*?loadAdFromHeaderTab.*?<\\/li>",
            "replace": ""
          },

          //
          // Eliminate ads blocks.
          //

          {
            "regex": "(?s)<div class=\"blockHtml.*?Traffic Junky.*?<\\/div>",
            "replace": ""
          },

          //
          // Eliminate player pause ad.
          //

          {
            "regex": "(?s)<div id=\"pb_block\">",
            "replace": "<div id=\"pb_block\" style=\"background:rgba(255,0,0,0)\">"
          },
          {
            "regex": "(?s)<div id=\"pb_top_bar\">.*?<\\/div>",
            "replace": "<div id=\"pb_top_bar\" style=\"background:rgba(255,0,0,0)\"></div>"
          },
          {
            "regex": "(?s)<div id=\"pb_content\">.*?<\\/div>",
            "replace": "<div id=\"pb_content\" style=\"background:rgba(255,0,0,0)\"></div>"
          },

          //
          // Correct bogus resume player button.
          //

          {
            "regex": "Zusammenfassung",
            "replace": "Fortsetzen"
          }
        ]
      }
    }
  },

  "www.pornhub.com": {
    "url": "http://www.pornhub.com/",
    "icon": "http://pbs.twimg.com/profile_images/615525585826222080/W9b7zw3R_400x400.jpg",
    "label": "Pornhub",
    "default": true,
    "subtype": "erotics",

    "subdomains": [ "phncdn.com" ],

    "webguard": {

      //
      // Site javascript crashes w/o this.
      //

      "feature": [ "DomStorage=1" ],

      //
      // Preset age check and table style.
      //

      "cookies": [ "age_verified=1", "platform=tablet" ],

      "mingle": {
        "replace": [

          //
          // Get rid of PornHub NetworkBar.
          //

          {
            "regex": "<div id=\"backgroundLayer\">",
            "replace": "<div id=\"backgroundLayer\" style=\"display:none\">"
          },
          {
            "regex": "<div id=\"pageLayer\">",
            "replace": "<div id=\"pageLayer\" style=\"top:0px\">"
          },

          //
          // Turn ads areas invisible.
          //

          {
            "regex": "class=\"rightAds\"",
            "replace": "class=\"rightAds\" style=\"display:none\""
          },
          {
            "regex": "class=\"ad\"",
            "replace": "class=\"ad\" style=\"display:none\""
          },

          //
          // Resize video player.
          //

          {
            "regex": "<div class=\"playerContainer\">",
            "replace": "<div class=\"playerContainer\" style=\"float:none;width:650px;height:478px;margin:auto\">"
          },

          //
          // Eliminate player pause ad.
          //

          {
            "regex": "(?s)<div id=\"pb_block\">",
            "replace": "<div id=\"pb_block\" style=\"background:rgba(255,0,0,0)\">"
          },
          {
            "regex": "(?s)<div id=\"pb_top_bar\">.*?<\\/div>",
            "replace": "<div id=\"pb_top_bar\" style=\"background:rgba(255,0,0,0)\"></div>"
          },
          {
            "regex": "(?s)<div id=\"pb_content\">.*?<\\/div>",
            "replace": "<div id=\"pb_content\" style=\"background:rgba(255,0,0,0)\"></div>"
          }
        ]
      }
    }
  },

